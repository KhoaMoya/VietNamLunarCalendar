<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.khoa.lunarcalendar">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:icon="@drawable/icon_calendar"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_calendar"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="m">
        <activity android:name=".calendar.view.activity.AddEnventActivity" />
        <activity android:name=".calendar.view.activity.EditEventActivity" />
        <activity android:name=".main.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>